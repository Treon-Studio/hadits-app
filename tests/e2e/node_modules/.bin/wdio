#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/@wdio+cli@9.15.0/node_modules/@wdio/cli/bin/node_modules:/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/@wdio+cli@9.15.0/node_modules/@wdio/cli/node_modules:/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/@wdio+cli@9.15.0/node_modules/@wdio/node_modules:/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/@wdio+cli@9.15.0/node_modules:/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/@wdio+cli@9.15.0/node_modules/@wdio/cli/bin/node_modules:/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/@wdio+cli@9.15.0/node_modules/@wdio/cli/node_modules:/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/@wdio+cli@9.15.0/node_modules/@wdio/node_modules:/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/@wdio+cli@9.15.0/node_modules:/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@wdio/cli/bin/wdio.js" "$@"
else
  exec node  "$basedir/../@wdio/cli/bin/wdio.js" "$@"
fi
