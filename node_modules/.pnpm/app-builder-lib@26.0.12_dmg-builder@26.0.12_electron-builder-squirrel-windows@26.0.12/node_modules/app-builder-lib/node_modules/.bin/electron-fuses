#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+fuses@1.8.0/node_modules/@electron/fuses/dist/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+fuses@1.8.0/node_modules/@electron/fuses/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+fuses@1.8.0/node_modules/@electron/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+fuses@1.8.0/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+fuses@1.8.0/node_modules/@electron/fuses/dist/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+fuses@1.8.0/node_modules/@electron/fuses/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+fuses@1.8.0/node_modules/@electron/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+fuses@1.8.0/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@electron+fuses@1.8.0/node_modules/@electron/fuses/dist/bin.js" "$@"
else
  exec node  "$basedir/../../../../../@electron+fuses@1.8.0/node_modules/@electron/fuses/dist/bin.js" "$@"
fi
