#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/astro@5.7.13_@types+node@22.15.18_encoding@0.1.13_jiti@2.4.2_lightningcss@1.30.1_rollup_8d9cbb1a5e0409ac282f90435cc345a1/node_modules/astro/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/astro@5.7.13_@types+node@22.15.18_encoding@0.1.13_jiti@2.4.2_lightningcss@1.30.1_rollup_8d9cbb1a5e0409ac282f90435cc345a1/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/astro@5.7.13_@types+node@22.15.18_encoding@0.1.13_jiti@2.4.2_lightningcss@1.30.1_rollup_8d9cbb1a5e0409ac282f90435cc345a1/node_modules/astro/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/astro@5.7.13_@types+node@22.15.18_encoding@0.1.13_jiti@2.4.2_lightningcss@1.30.1_rollup_8d9cbb1a5e0409ac282f90435cc345a1/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
