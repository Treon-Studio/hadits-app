#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@lynx-js+rspeedy@0.9.5_@rspack+core@1.3.10_@swc+helpers@0.5.17__typescript@5.7.3_webpack@5.99.8/node_modules/@lynx-js/rspeedy/bin/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@lynx-js+rspeedy@0.9.5_@rspack+core@1.3.10_@swc+helpers@0.5.17__typescript@5.7.3_webpack@5.99.8/node_modules/@lynx-js/rspeedy/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@lynx-js+rspeedy@0.9.5_@rspack+core@1.3.10_@swc+helpers@0.5.17__typescript@5.7.3_webpack@5.99.8/node_modules/@lynx-js/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@lynx-js+rspeedy@0.9.5_@rspack+core@1.3.10_@swc+helpers@0.5.17__typescript@5.7.3_webpack@5.99.8/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@lynx-js+rspeedy@0.9.5_@rspack+core@1.3.10_@swc+helpers@0.5.17__typescript@5.7.3_webpack@5.99.8/node_modules/@lynx-js/rspeedy/bin/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@lynx-js+rspeedy@0.9.5_@rspack+core@1.3.10_@swc+helpers@0.5.17__typescript@5.7.3_webpack@5.99.8/node_modules/@lynx-js/rspeedy/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@lynx-js+rspeedy@0.9.5_@rspack+core@1.3.10_@swc+helpers@0.5.17__typescript@5.7.3_webpack@5.99.8/node_modules/@lynx-js/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@lynx-js+rspeedy@0.9.5_@rspack+core@1.3.10_@swc+helpers@0.5.17__typescript@5.7.3_webpack@5.99.8/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@lynx-js/rspeedy/bin/rspeedy.js" "$@"
else
  exec node  "$basedir/../@lynx-js/rspeedy/bin/rspeedy.js" "$@"
fi
