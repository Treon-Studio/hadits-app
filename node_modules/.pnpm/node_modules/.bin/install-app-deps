#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/electron-builder@26.0.12_electron-builder-squirrel-windows@26.0.12/node_modules/electron-builder/node_modules:/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/electron-builder@26.0.12_electron-builder-squirrel-windows@26.0.12/node_modules:/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/electron-builder@26.0.12_electron-builder-squirrel-windows@26.0.12/node_modules/electron-builder/node_modules:/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/electron-builder@26.0.12_electron-builder-squirrel-windows@26.0.12/node_modules:/Users/ridho/Documents/go/github.com/raizora/muslimfy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../electron-builder/install-app-deps.js" "$@"
else
  exec node  "$basedir/../electron-builder/install-app-deps.js" "$@"
fi
