#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules/@electron/windows-sign/bin/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules/@electron/windows-sign/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules/@electron/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules/@electron/windows-sign/bin/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules/@electron/windows-sign/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules/@electron/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules:/Users/ridho/Documents/go/github.com/raizora/hadits-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@electron+windows-sign@1.2.2/node_modules/@electron/windows-sign/bin/electron-windows-sign.js" "$@"
else
  exec node  "$basedir/../../../../../@electron+windows-sign@1.2.2/node_modules/@electron/windows-sign/bin/electron-windows-sign.js" "$@"
fi
